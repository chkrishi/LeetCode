#My own soln

class Solution:
    def hasPathSum(self, root: Optional[TreeNode], targetSum: int) -> bool:
        if not root:
            return False
        def dfs(root , val):
            if not root:
                return 
            val += root.val
            if val == targetSum and not root.left and not root.right:
                return True
            else:
                return dfs(root.left, val) or dfs(root.right, val)
        if dfs(root, 0): return True
        return False
