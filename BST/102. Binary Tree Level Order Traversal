class Solution:
    def levelOrder(self, root: Optional[TreeNode]) -> List[List[int]]:
        if not root:
            return []
        stk = deque([root])
        arr = []
        while stk:
            temp = []
            for i in range(len(stk)):
                node = stk.popleft()
                temp.append(node.val)
                if node.left:
                    stk.append(node.left)
                if node.right:
                    stk.append(node.right)
            arr.append(temp)
        return arr
