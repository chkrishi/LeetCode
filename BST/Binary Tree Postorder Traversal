class Solution:
    def postorderTraversal(self, root: Optional[TreeNode]) -> List[int]:
        if not root:
            return []
        val = []
        stk = deque([root])
        while stk:
            temp = stk.pop()
            val.append(temp.val)
            if temp.left:
                stk.append(temp.left)
            if temp.right:
                stk.append(temp.right)
        return val[::-1]
