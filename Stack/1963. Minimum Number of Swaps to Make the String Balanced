#My own soln but TLE --> 55/59

class Solution:
    def minSwaps(self, s: str) -> int:
        tempval = '[]'
        templenval = -1
        while templenval != len(s):
            templenval = len(s)
            s = s.replace(tempval,'')
        count = 0
        count = min(s.count('['),s.count(']'))
        return (count + 1) // 2
