# Modified Binary search
class Solution:
    def findMin(self, nums: List[int]) -> int:
        i,j = 0 , len(nums) - 1
        while i < j:
            if nums[i] > nums[i+1]: # FOUND THE ROTATION POINT
                return nums[i+1]
            elif nums[j] < nums[j-1]: # FOUND THE ROTATION POINT
                return nums[j]
            i += 1
            j -= 1
        return nums[0]
