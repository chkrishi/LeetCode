class Solution:
    def search(self, nums: List[int], target: int) -> int:
        i,j = 0, len(nums) - 1
        while i <= j :
            midval = (i + j) // 2
            if target == nums[midval]:
                return midval
            elif nums[i] <= nums[midval]:
                if nums[i] <= target < nums[midval]:
                    j = midval - 1
                else:
                    i = midval + 1
            else:
                if nums[midval] < target <= nums[j]:
                    i = midval + 1
                else:
                    j = midval - 1
            
        return -1
        
