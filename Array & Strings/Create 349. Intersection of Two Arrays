#Editorial Soln
class Solution(object):
    def intersection(self, nums1, nums2):
        s_nums1 = set(nums1)
        return list(s_nums1.intersection(nums2))

#My Soln

class Solution(object):
    def intersection(self, nums1, nums2):
        arr = []
        for i in range(0,len(nums1)):
            if nums1[i] in nums2:
                arr.append(nums1[i])
        x = set(arr)
        return list(x)
